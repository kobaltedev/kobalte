{
  "name": "kobalte-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "A UI toolkit for building SolidJS applications.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobaltedev/kobalte.git"
  },
  "license": "MIT",
  "author": "jer3m01 <jer3m01@jer3m01.com>",
  "contributors": [
    "Fabien Marie-Louise <fabienml.dev@gmail.com>"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "nx run-many -t build",
    "build:libs": "pnpm build --exclude @kobalte/docs",
    "changeset": "changeset",
    "nx:release": "nx release --dry-run",
    "ci:release": "pnpm build && changeset publish",
    "ci:version": "changeset version && pnpm i --no-frozen-lockfile && git add .",
    "clean": "nx run-many -t clean && rm -rf node_modules",
    "commit": "git-cz",
    "dev": "nx dev @kobalte/core",
    "dev:docs": "nx dev @kobalte/docs",
    "format": "biome check --write && prettier . --write",
    "check": "biome check",
    "test": "nx run-many -t test --output-style stream",
    "typecheck": "nx run-many -t typecheck"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@biomejs/biome": "1.8.1",
    "@changesets/cli": "2.26.2",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@commitlint/cz-commitlint": "17.7.1",
    "@nx/js": "19.7.2",
    "@solidjs/testing-library": "0.8.6",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.5.4",
    "@vitest/ui": "^1.5.2",
    "babel-preset-solid": "1.7.7",
    "commitizen": "4.3.0",
    "inquirer": "8.2.5",
    "jsdom": "19.0.0",
    "nx": "19.7.2",
    "prettier": "4.0.0-alpha.8",
    "prettier-plugin-tailwindcss": "0.5.3",
    "rollup": "3.28.1",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-license": "3.2.0",
    "rollup-preset-solid": "2.0.1",
    "solid-js": "1.8.15",
    "tsup": "7.2.0",
    "turbo": "1.10.13",
    "typescript": "4.9.5",
    "vite": "5.0.11",
    "vite-plugin-solid": "2.9.1",
    "vitest": "1.3.1"
  },
  "packageManager": "pnpm@8.14.0",
  "pnpm": {
    "patchedDependencies": {
      "nx@19.7.2": "patches/nx@19.7.2.patch"
    }
  }
}
